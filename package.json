{
  "name": "securite-electronique-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "fix-code": "node scripts/fix-eslint.js",
    "quality-check": "npm run type-check && npm run lint",
    "test:features": "node scripts/test-features.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "docker:build": "docker build -t itvision-app .",
    "docker:dev": "docker-compose --env-file .env.docker up --build",
    "docker:prod": "docker-compose --env-file .env.docker -f docker-compose.yml up -d --build",
    "docker:stop": "docker-compose --env-file .env.docker down",
    "docker:logs": "docker-compose --env-file .env.docker logs -f",
    "docker:backup": "docker-compose --env-file .env.docker --profile backup run --rm backup"
  },
  "dependencies": {
    "@next/font": "^14.2.15",
    "@prisma/client": "^6.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/nodemailer": "^7.0.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "framer-motion": "^12.23.12",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.536.0",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "prisma": "^6.17.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
